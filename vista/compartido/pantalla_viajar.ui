   <property name="styleSheet">
    <string notr="true">QTableView {
    background: #ffffff;
    border: 1px solid #e6e8ec;
    border-radius: 8px;
    gridline-color: #f1f1f3;
    selection-background-color: #e8f0fe;
    selection-color: #1a1a1a;
    font-size: 14px;
    color: #333;
    alternate-background-color: #fafbff;
    show-grid: false;
    
    /* Centrar todo el contenido de las celdas */
    qproperty-alignment: 'AlignCenter';
}

QTableView::item {
    padding: 8px 12px;
    border: 0px;
    
    /* Centrar contenido de cada item */
    text-align: center;
}

QTableView::item:hover {
    background: #f5f8ff;
}

QTableView::item:selected {
    background: #e2edff;
    color: #1a1a1a;
}

QHeaderView::section {
    background: #ffffff;
    color: #6b7280;
    padding: 10px 12px;
    border: 0;
    border-bottom: 2px solid #e6e8ec;
    font-size: 17px;
    font-weight: 600;
    
    /* Centrar texto del encabezado */
    text-align: center;
    
    /* Asegurar que todas las columnas tengan el mismo ancho */
    qproperty-minimumSectionSize: 150; /* Ancho mínimo igual para todas */
    qproperty-defaultAlignment: 'AlignCenter'; /* Centrar texto del header */
}

QHeaderView::section:hover {
    background: #f7f9fc;
}

QHeaderView {
    background: #ffffff;
    border: 0;
    
    /* IMPORTANTE: Forzar que las secciones se estiren para llenar el espacio */
    qproperty-stretchLastSection: false;
    qproperty-resizeMode: 'Stretch'; /* Esto hace que las columnas se ajusten automáticamente */
}

/* Configuración adicional para el modelo de datos en Python */
QTableView QAbstractItemView {
    /* Asegurar que las columnas se ajusten al contenido */
    qproperty-horizontalScrollMode: 'ScrollPerPixel';
    qproperty-verticalScrollMode: 'ScrollPerPixel';
}

QTableView::indicator {
    width: 18px;
    height: 18px;
    /* Centrar el checkbox si hay */
    margin-left: auto;
    margin-right: auto;
}

QHeaderView::section:first {
    border-top-left-radius: 8px;
}

QHeaderView::section:last {
    border-top-right-radius: 8px;
}

QScrollBar:vertical {
    background: transparent;
    width: 10px;
    margin: 0px;
}

QScrollBar::handle:vertical {
    background: #cfd3da;
    min-height: 25px;
    border-radius: 5px;
}

QScrollBar::handle:vertical:hover {
    background: #b5bac4;
}

QScrollBar::add-line,
QScrollBar::sub-line {
    background: none;
    height: 0px;
}</string>
